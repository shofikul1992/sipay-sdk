        <activity android:name=".QRCodeActivity" />
        <activity android:name=".zxing.android.BackActivity" />
        <activity android:name=".zxing.android.CaptureActivity" />
        <activity android:name=".zxing.android.FontActivity" />
        <activity android:name=".APITestActivity" />
